<rt-require dependency="material-ui" as="mui"/>
<rt-require dependency="../loginBox/loginBox" as="loginBox"/>
<rt-require dependency="../createAccountBox/createAccountBox" as="createAccountBox"/>
<rt-require dependency="../forgotPasswordBox/forgotPasswordBox" as="forgotPasswordBox"/>

<div class="auth-box">
    <loginBox rt-if="this.state.showLoginBox"/>
    <createAccountBox rt-if="this.state.showCreateAccountBox"/>
    <forgotPasswordBox rt-if="this.state.showForgotPasswordBox" />
</div>