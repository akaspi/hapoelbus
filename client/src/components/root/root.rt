<rt-require dependency="../myAccount/myAccount" as="myAccount"/>
<rt-require dependency="../loginBox/loginBox" as="loginBox"/>

<div>
    <div rt-if="this.state.isLoggedIn" key="key-my-account">
        <myAccount/>
    </div>
    <div rt-if="!this.state.isLoggedIn" key="key-login-box">
        <loginBox />
    </div>
</div>
