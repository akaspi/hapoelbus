<rt-require dependency="material-ui" as="mui"/>

<div class="navigation">
    <i class="material-icons menu-icon" onClick="{this.toggleNav}">menu</i>

    <mui.LeftNav
            ref="nav"
            docked="{false}"
            menuItems="{this.getNavItems()}"
            onChange="{this.onNavChange}"
            value="{this.state.selectedTab}"/>
</div>