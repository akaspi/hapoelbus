<rt-require dependency="../accountData/accountData" as="accountData"/>
<rt-require dependency="../dashboard/dashboard" as="dashboard"/>

<div class="my-account">
    <div rt-if="this.props.isFetchingUserData" key="key-loading-my-account">Loading....</div>
    <accountData rt-if="!this.props.isFetchingUserData && !this.props.userData"  key="key-account-data"/>
    <dashboard rt-if="!this.props.isFetchingUserData && this.props.userData" key="key-dashboard" userData="{this.props.userData}">
</div>