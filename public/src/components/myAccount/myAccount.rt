<rt-require dependency="../accountData/accountData" as="accountData"/>

<div>
    <div rt-if="this.state.isFetchingInitialData">Loading....</div>
    <div rt-if="!this.state.isFetchingInitialData && !this.hasAccount()" class="create-account">
        <accountData />
    </div>
    <div rt-if="!this.state.isFetchingInitialData && this.hasAccount()" class="dashboard">Dashboard!!!!!!! :)
        <button onClick="{this.logout}">Logout</button>
    </div>
</div>