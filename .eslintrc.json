{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jasmine": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "comma-dangle": 0,
        "func-names": 0,
        "eol-last": 0,
        "prefer-template": 0,
        "max-len": 0,
        "react/prefer-es6-class": 0,
        "padded-blocks": 0,
        "react/sort-comp": 0
    }
}
